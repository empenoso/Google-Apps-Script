// –°–∫–ª–æ–Ω—è–µ–º –æ–∫–æ–Ω—á–∞–Ω–∏–µ —Å–æ–≥–ª–∞—Å–Ω–æ –≤–æ–∑—Ä–∞—Å—Ç–∞ (–ª–µ—Ç, –≥–æ–¥, –≥–æ–¥–∞) –¥–ª—è –¥–Ω–µ–π —Ä–æ–∂–¥–µ–Ω–∏—è
function text(age) {
    var years;
    count = age % 100;
    if (count >= 5 && count <= 20) {
        years = '–ª–µ—Ç';
    } else {
        count = count % 10;
        if (count == 1) {
            years = '–≥–æ–¥';
        } else if (count >= 2 && count <= 4) {
            years = '–≥–æ–¥–∞';
        } else {
            years = '–ª–µ—Ç';
        }
    }
    return years;
}

// –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Å–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–µ —Ç—Ä–∏–≥–≥–µ—Ä—ã –¥–ª—è –∑–∞–ø—É—Å–∫–∞
function TriggersCreateTimeDriven() {
    // –£–¥–∞–ª—è–µ—Ç –≤—Å–µ —Ç—Ä–∏–≥–≥–µ—Ä—ã –≤ —Ç–µ–∫—É—â–µ–º –ø—Ä–æ–µ–∫—Ç–µ
    var triggers = ScriptApp.getProjectTriggers();
    for (var i = 0; i < triggers.length; i++) {
        ScriptApp.deleteTrigger(triggers[i]);
    }

    // –¢–µ–ø–µ—Ä—å —Å–æ–∑–¥–∞–µ–º –¥–ª—è –¥–Ω–µ–π —Ä–æ–∂–¥–µ–Ω–∏—è
    ScriptApp.newTrigger("birthdayAgeToCalendar")
        .timeBased()
        .onMonthDay(1) // –¥–µ–Ω—å –º–µ—Å—è—Ü–∞
        .atHour(1) // —á–∞—Å
        .create();

    // –¢–µ–ø–µ—Ä—å —Å–æ–∑–¥–∞–µ–º –¥–ª—è –≥–æ–¥–æ–≤—â–∏–Ω, —é–±–∏–ª–µ–µ–≤     
    ScriptApp.newTrigger("anniversaryAgeToCalendar")
        .timeBased()
        .onMonthDay(1) // –¥–µ–Ω—å –º–µ—Å—è—Ü–∞
        .atHour(2) // —á–∞—Å
        .create();
}

function logToDrive() { //—Å–æ–∑–¥–∞–µ–º —Ñ–∞–π–ª –ª–æ–≥–∞ –Ω–∞ –¥–∏—Å–∫–µ    
    var id = ScriptApp.getScriptId();
    var name = DriveApp.getFileById(id).getName();
    // –æ–ø—Ä–µ–¥–µ–ª—è–µ–º –∏–º—è –ø–∞–ø–∫–∏ - –Ω–∞—á–∞–ª–æ
    var file = DriveApp.getFileById(id);
    var folders = file.getParents();
    while (folders.hasNext()) {
        var folder_name = folders.next().getName();
        Logger.log("logToDrive. –ò–º—è –ø–∞–ø–∫–∏: " + folder_name)
    }
    // –æ–ø—Ä–µ–¥–µ–ª—è–µ–º –∏–º—è –ø–∞–ø–∫–∏ - –∫–æ–Ω–µ—Ü
    var fileName = name + "_GoogleAppsLog.txt";
    try {
        var dir = DriveApp.getFoldersByName(folder_name).next(); //–µ—Å–ª–∏ –≤ –∫–∞–∫–æ–π-—Ç–æ –ø–∞–ø–∫–µ
    } catch (error) {
        var dir = DriveApp.getRootFolder(); //–µ—Å–ª–∏ –∫–æ—Ä–µ–Ω—å –¥–∏—Å–∫–∞
    }

    var files = dir.getFiles();
    while (files.hasNext()) {
        var file = files.next();
        Logger.log("logToDrive. –§–∞–π–ª—ã –≤ –ø–∞–ø–∫–µ: " + file.getName())
        if (file.getName() === fileName) {
            file.setTrashed(true); //—É–¥–∞–ª—è–µ–º –ø—Ä–µ–¥—ã–¥—É—â–∏–π –ª–æ–≥ —Ñ–∞–π–ª
            break;
        }
    }
    var file = dir.createFile(fileName, Logger.getLog()); //—Å–æ–∑–¥–∞–µ–º –ª–æ–≥ —Ñ–∞–π–ª
}
